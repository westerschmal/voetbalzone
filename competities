	<script>
	var cmpResponse = {};
	var consentForAds;
	function processCmpConsent() {
		if (cmpResponse.getConsentData && cmpResponse.getConsentData.consentData) {
			var newConsentForAds = (cmpResponse.getVendorConsents.purposeConsents[1] && cmpResponse.getVendorConsents.purposeConsents[2] && cmpResponse.getVendorConsents.purposeConsents[3] && cmpResponse.getVendorConsents.purposeConsents[4] && cmpResponse.getVendorConsents.purposeConsents[5]);
			if (consentForAds !== newConsentForAds) {
				consentForAds = newConsentForAds;
				//code below should be uncommented if we only want to load ads when a user has given consent for all purposes:
				//if (consentForAds) {
				googletag.cmd.push(function() {
						//send IAB string to DFP, can be used in tags using macro %%PATTERN:iab_string%%
					googletag.pubads().setTargeting('iab_string', [cmpResponse.getConsentData.consentData]);
					googletag.pubads().setTargeting('consent', [consentForAds ? 1 : 0]);
					//request non-personalized ads from Google if user does not give consent.
					googletag.pubads().setRequestNonPersonalizedAds(consentForAds ? 0 : 1);
					//release DFP when consent values are available
					defineAdUnits();
					//Load Criteo anti-adblock script
					//try { loadCriteo(); } catch(e) {};
				})
		   // }
		}
		}
	};
	function handleCmpResponseCallbacks() {
		function afterEach() { if (cmpResponse.getConsentData && cmpResponse.getVendorConsents) { processCmpConsent() } }
		return {
			consentDataCallback: function(consentResponse) {
				cmpResponse.getConsentData = consentResponse;
				afterEach()
			},
			vendorConsentsCallback: function(consentResponse) {
				cmpResponse.getVendorConsents = consentResponse;
				afterEach()
			}
		}
	}
	var callbackHandler = handleCmpResponseCallbacks();
	window.__cmp('getConsentData', null, callbackHandler.consentDataCallback);
	window.__cmp('getVendorConsents', null, callbackHandler.vendorConsentsCallback);
	</script>
